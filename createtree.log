BiTree Create(BiTree T)   //create tree
{
   char ch;
   ch=getchar();
   if(ch=='#')
   T=NULL;
   else
   {
   if(!(T=(BiTNode *)malloc(sizeof(BiTNode))))
   printf("Error!");
   T->data=ch;
   T->lchild=Create(T->lchild);
   T->rchild=Create(T->rchild);
   }
   return T;
}

